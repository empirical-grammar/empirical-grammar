<script>
document.addEventListener('keydown', function(e) {
  if (e.keyCode === 9) {
    $('body').addClass('show-focus');
  }
});
document.addEventListener('click', function(e) {
  console.log("clicked")
  $('body').removeClass('show-focus');
});
</script>
