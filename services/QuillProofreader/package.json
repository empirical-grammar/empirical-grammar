{
  "name": "quillproofreader",
  "version": "1.0.0",
  "description": "Quill Proofreader React App",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --exclude 'src/tests/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --exclude 'src/tests/*.{ts,tsx}' --fix",
    "start": "EMPIRICAL_BASE_URL=localhost:3000 webpack-dev-server",
    "test": "mocha --require ts-node/register src/tests/**/*.spec.ts",
    "jest:u": "jest --updateSnapshot",
    "jest": "jest",
    "build:jenkins": "tsc && webpack --optimize-minimize",
    "build": "npm install && tsc && webpack --progress",
    "build:staging": "npm install && webpack --progress --config webpack.staging.config.js",
    "build:prod": "npm install && webpack --progress --config webpack.prod.config.js",
    "clean": "rd /s/q built"
  },
  "keywords": [
    "quill",
    "proofreader",
    "quillproofreader"
  ],
  "author": "Quill.org",
  "license": "AGPL-3.0",
  "dependencies": {
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/immutable": "^3.8.7",
    "@types/lodash": "^4.14.109",
    "@types/node": "^9.4.7",
    "@types/react": "^15.6.28",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/react-router": "^4.0.23",
    "@types/react-router-config": "^1.0.6",
    "@types/react-router-dom": "^4.2.5",
    "@types/react-select": "^2.0.3",
    "@types/request": "^2.47.0",
    "@types/underscore": "^1.8.9",
    "antd": "^4.1.4",
    "bulma": "^0.7.1",
    "css-hot-loader": "^1.3.9",
    "draft-convert": "^2.1.4",
    "enzyme-adapter-react-16": "^1.15.2",
    "firebase": "^7.14.1",
    "fs": "0.0.1-security",
    "immutable": "^3.8.2",
    "jsdiff": "^1.1.1",
    "lodash": "^4.17.15",
    "net": "^1.0.2",
    "node-sass": "^4.14.1",
    "quill-component-library": "0.7.35",
    "quill-marking-logic": "0.13.7",
    "quill-string-normalizer": "0.0.9",
    "react": "^16.13.1",
    "react-contenteditable": "^3.3.4",
    "react-dom": "^16.13.1",
    "react-input-autosize": "^2.2.2",
    "react-redux": "^5.0.7",
    "react-router": "^5.1.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "react-select": "^2.0.0",
    "react-sortable": "^2.0.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.87.0",
    "sass": "^1.3.2",
    "sass-loader": "^7.0.1",
    "sbd": "^1.0.18",
    "style-loader": "^0.21.0",
    "tls": "0.0.1",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.1",
    "@types/jest": "^22.2.0",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.0.0",
    "enzyme": "^3.1.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^3.0.7",
    "jest": "^24.8.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.2.0",
    "mocha": "^7.1.1",
    "react-test-renderer": "^16.0.0",
    "redux-devtools-extension": "^2.13.2",
    "source-map-loader": "^0.2.3",
    "ts-import-plugin": "^1.4.3",
    "ts-jest": "^25.5.1",
    "ts-loader": "^4.1.0",
    "ts-node": "^8.8.2",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.7.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.11.0",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "testRegex": "\\.(test|spec)\\.(ts|tsx|js)$",
    "setupFiles": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testURL": "http://localhost:8070"
  }
}
