= render partial: 'pages/shared/pages_tabs'
= render partial: 'pages/shared/about_subtabs'

.container.pages-container

  h3.page-subtitle2
  - (team_info).each do |team_type|
    .row
    h3 #{team_type[:team]}
    - team_type[:members].in_groups_of(3) do |people|
          .row
          - people.each do |person|
            - if person
              .col-md-4.col-xs-1.team_section
                =image_tag person[:img]
                h4 =person[:name]
                p =person[:title]
                p =person[:desc]
